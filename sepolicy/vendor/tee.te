typeattribute tee data_between_core_and_vendor_violators;

# /data/goodix labeling
type_transition tee system_data_file:{ dir file } gx_fpd_data_file;

allow tee system_data_file:dir { open getattr read search };
allow tee fingerprintd_data_file:dir { open getattr read search lock write add_name remove_name create reparent rename rmdir setattr };
allow tee fingerprintd_data_file:file { open getattr read lock write create rename setattr };
allow tee unlabeled:dir search;

# allow storing goodix data
#allow tee system_file:file { getattr open read ioctl lock execute execute_no_trans };
allow tee mnt_vendor_file:dir search;
allow tee { vendor_persist_drm_file gx_fpd_data_file }:file { open getattr read lock write create rename setattr };
allow tee { vendor_persist_drm_file gx_fpd_data_file }:dir { open getattr read search lock write add_name remove_name create reparent rename rmdir setattr };
allow tee system_data_file:dir { open getattr read search lock write add_name remove_name create reparent rename rmdir setattr };