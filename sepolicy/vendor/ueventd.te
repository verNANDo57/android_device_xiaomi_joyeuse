typeattribute ueventd data_between_core_and_vendor_violators;

allow ueventd self:capability sys_nice;
allow vendor_qti_init_shell sysfs_wakeup:file setattr;
allow ueventd fpc_sysfs:file { getattr open read lock append write };
allow ueventd vendor_wifi_vendor_data_file:dir { open getattr read search };
allow ueventd vendor_wifi_vendor_data_file:file { getattr open read lock };
allow ueventd debugfs:dir { open getattr read search };

# KCAL
allow ueventd kcal_dev:dir { open getattr read search };
allow ueventd kcal_dev:file { getattr open read lock append write };
allow ueventd kcal_dev:lnk_file { getattr open read lock };